{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/static/*",
        "/images/*",
        "/favicon.ico"
      ]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }